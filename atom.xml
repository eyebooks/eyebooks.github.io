</span><span class="line">	<span class="k">let</span> <span class="n">local</span> <span class="o">=</span> <span class="n">internal</span>
</span><span class="line"><span class="p">}</span>
</span><span class="line"><span class="k">func</span> <span class="nf">bar</span><span class="p">()</span> <span class="p">{</span>
</span><span class="line">	<span class="k">let</span> <span class="n">result</span> <span class="o">=</span> <span class="n">foo</span><span class="p">(</span><span class="mi">123</span><span class="p">)</span>
